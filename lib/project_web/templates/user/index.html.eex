<h1 class="m-2">List of actual users in this app </h1>

<table class="m-2">
  <thead>
    <tr>
      <th>Email</th>    
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
<%= for user <- @users do %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.name %></td>
      <td>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "Create New User", to: Routes.user_path(@conn, :new) %></span>
