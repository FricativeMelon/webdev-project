<h1>Show Notification</h1>

<ul>

  <li>
    <strong>User:</strong>
    <%= @notification.user_id %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @notification.description %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.notification_path(@conn, :edit, @notification) %></span>
<span><%= link "Back", to: Routes.notification_path(@conn, :index) %></span>
